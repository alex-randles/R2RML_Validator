@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix testv: <http://www.txample.com/people/voc/>.
@prefix testd: <http://www.txample.com/people/data/>.


<#Test>
 
	rr:logicalTable [ rr:sqlQuery "";];
		
	rr:subjectMap [
		rr:template "http://www.example.com/people/data/{ID}";
		rr:termType rr:IRI;
		rr:class "http://www.example.com/" ;
	];
	
	rr:predicateObjectMap [
		rr:predicate testv:firstName; 
					rr:datatype xsd:decimal;

		rr:objectMap  [ 
			rr:column "FNAME";
			rr:termType rr:IRI;
			rr:language "en-us" ;

			rr:datatype xsd:positiveInteger;

		];		
   ];
   
   rr:predicateObjectMap [
		rr:predicate testv:lastName ;
		rr:objectMap  [ 
        rr:parentTriplesMap <#TriplesMap2>;
        rr:joinCondition [
            rr:child "";
            rr:parent "DEPTNO";
        ];
		];		
   ];
   
   rr:predicateObjectMap [
		rr:predicate testv:location ;
		rr:objectMap  [ 
			rr:column "LOC";
			rr:termType rr:Literal;
		];		
   ];
   
   rr:predicateObjectMap [
		rr:predicate testv:age ;
		rr:objectMap  [ 
			#rr:template "http://www.example.com/people/data/{AGE}";
			rr:column "rfg";
			rr:termType rr:Literal;
			#rr:termType rr:IRI;
		];		
   ];
   
   
.
