@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix testv: <http://www.txample.com/people/voc/>.
@prefix testd: <http://www.txample.com/people/data/>.


<#Test>
 
	## EMPTY TABLE NAME ## 
	rr:logicalTable [ rr:tableName ""];
		
	rr:subjectMap [
		rr:template "http://www.example.com/people/data/{ID}";
		rr:termType rr:IRI;
		rr:class "http://www.example.com/" ;
	];
	
	rr:predicateObjectMap [
		rr:predicate testv:firstName; 
		rr:objectMap  [ 
			rr:column "FNAME";
			rr:termType rr:IRI;
			
			## TWO DATATYPES FOR ONE OBJECT ##
			rr:datatype xsd:decimal;
		    rr:datatype xsd:integer;

		];		
   ];
   
    rr:predicateObjectMap [
		rr:predicate testv:location ;
		rr:objectMap  [ 
			rr:column "LOC";
			
			## INVALID TERM TYPE ##
			rr:termType rr:tableName;
		];		
   ];
   
   rr:predicateObjectMap [
		rr:predicate testv:lastName ;
		rr:objectMap  [ 
        rr:parentTriplesMap <#TriplesMap2>;
        rr:joinCondition [
            rr:child "fff";
            rr:parent "DEPTNO";
        ];
		];		
   ];
   

   
   rr:predicateObjectMap [
		rr:predicate testv:age ;
		rr:objectMap  [ 
			#rr:template "http://www.example.com/people/data/{AGE}";
			rr:column "rfg";
			rr:termType rr:Literal;
			#rr:termType rr:IRI;
		];		
   ];
   
   
.
