<#Test>

	rr:logicalTable [ rr:tableName "test";];

	rr:subjectMap [
		rr:template "http://www.example.com/people/data/{ID}";
		rr:termType rr:IRI;
		rr:class "http://www.example.com/" ;
	];
	
   rr:predicateObjectMap [
		rr:predicate foaf:knows;
		rr:objectMap  [ 
			rr:column "name";
			rr:termType rr:Literal;
		];
   ];

	rr:predicateObjectMap [
		rr:predicate dbo:age ;
		rr:objectMap  [
			rr:column "age";
			rr:termType rr:Literal;
